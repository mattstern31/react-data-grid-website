{
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "pluginSearchDirs": false,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "^node:",
    "^react$",
    "^react-dom",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "^@linaria/core$",
    "^clsx$",
    "",
    "./src",
    "./renderers",
    "./components",
    "./hooks",
    "./utils",
    "./types",
    "^\\.$",
    "^\\./",
    "^\\.\\./"
  ]
}
