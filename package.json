{
  "name": "react-data-grid",
  "version": "7.0.0-canary.7",
  "license": "MIT",
  "description": "Excel-like grid component built with React, with editors, keyboard navigation, copy & paste, and the like",
  "keywords": [
    "react",
    "react-data-grid",
    "data-grid"
  ],
  "repository": "github:adazzle/react-data-grid",
  "bugs": {
    "url": "https://github.com/adazzle/react-data-grid/issues"
  },
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "start": "node webpack-dev-server.mjs",
    "build": "tsc",
    "postbuild": "node tools/buildStylesheets.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "eslint": "eslint --ext js,mjs,ts,tsx --max-warnings 0 -f codeframe --cache --color examples src test tests tools",
