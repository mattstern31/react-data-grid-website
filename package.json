    "faker": "^4.1.0",
    "jest": "^25.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.13.1",
    "react-contextmenu": "^2.13.0",
