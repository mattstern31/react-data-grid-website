    "react-dom": "^16.13.1",
    "react-select": "^3.1.0",
    "react-virtualized": "^9.21.2",
    "ts-jest": "^25.3.1",
    "ts-loader": "^7.0.0",
    "typescript": "~3.8.3"
  },
  "peerDependencies": {
    "react": "^16.8",
    "react-dom": "^16.8"
  }
