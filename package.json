    "react": "^16.12.0",
    "react-contextmenu": "^2.13.0",
    "react-dnd-test-backend": "^2.6.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-virtualized": "^9.21.2",
    "recharts": "^1.7.1",
